<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/css/dataTables/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/dataTables/dataTables.responsive.css" rel="stylesheet" />
    <link href="~/Content/css/timeline.css" rel="stylesheet" />
    <link href="~/Content/css/startmin.css" rel="stylesheet" />
    <link href="~/Content/css/morris.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
</head>
    <body>
        <div id="wrapper">
            <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                <div class="navbar-header">
                    <a class="navbar-brand" href="index.html">Startmin</a>
                </div>

                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <ul class="nav navbar-nav navbar-left navbar-top-links">
                    <li><a href="#"><i class="fa fa-home fa-fw"></i> Website</a></li>
                </ul>

                <ul class="nav navbar-right navbar-top-links">

                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-user fa-fw"></i> @Session["UserName"] <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu dropdown-user">

                            <li class="divider"></li>
                            <li>
                                <a href="@Url.Action("LogOut","Home")"><i class="fa fa-sign-out fa-fw"></i> Logout</a>
                            </li>
                        </ul>
                    </li>
                </ul>
                <!-- /.navbar-top-links -->

                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <ul class="nav" id="side-menu">
                            <li class="sidebar-search">
                                <div class="input-group custom-search-form">
                                    <input type="text" class="form-control" placeholder="Search...">
                                    <span class="input-group-btn">
                                        <button class="btn btn-primary" type="button">
                                            <i class="fa fa-search"></i>
                                        </button>
                                    </span>
                                </div>
                                <!-- /input-group -->
                            </li>
                            <li>
                                <a href="@Url.Action("Dashboard","Home")" class="active"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                            </li>

                            <li>
                                <a href="@Url.Action("Tables","Home")"><i class="fa fa-table fa-fw"></i> Tables</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Form","Home")"><i class="fa fa-edit fa-fw"></i> Forms</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            @RenderBody()
        </div>
        <script src="~/Scripts/js/jquery.min.js"></script>
        <script src="~/scripts/js/bootstrap.min.js"></script>
        <script src="~/Scripts/js/metisMenu.min.js"></script>
        <script src="~/Scripts/js/dataTables/dataTables.bootstrap.min.js"></script>
        <script src="~/Scripts/js/dataTables/jquery.dataTables.min.js"></script>
        <script src="~/Scripts/js/raphael.min.js"></script>
       <script src="~/Scripts/js/morris.min.js"></script>
            <script src="~/Scripts/js/morris-data.js"></script>
        <script src="~/Scripts/js/startmin.js"></script>
        <div class="container body-content">
          
        </div>
    </body>
</html>
